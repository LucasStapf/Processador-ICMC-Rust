<interface>
  <!-- interface-description Utilizado para mostrar o conteúdo da memória do Processador. -->
  <requires lib="gtk" version="4.12"/>
  <template class="MemoryView" parent="GtkBox">
    <property name="orientation">vertical</property>
    <property name="valign">center</property>
    <child>
      <object class="GtkScrolledWindow" id="scrolled_memory_cells">
        <property name="css-classes">card</property>
        <property name="focusable">True</property>
        <property name="halign">center</property>
        <property name="hexpand">True</property>
        <property name="hscrollbar-policy">never</property>
        <property name="margin-end">4</property>
        <property name="margin-start">4</property>
        <property name="propagate-natural-height">True</property>
        <property name="propagate-natural-width">True</property>
        <property name="valign">center</property>
        <property name="vexpand">True</property>
        <property name="vscrollbar-policy">always</property>
        <child>
          <object class="GtkBox" id="box_memory_cells">
            <property name="orientation">vertical</property>
            <child>
              <object class="MemoryCellRow"/>
            </child>
            <child>
              <object class="MemoryCellRow"/>
            </child>
            <child>
              <object class="MemoryCellRow"/>
            </child>
            <child>
              <object class="MemoryCellRow"/>
            </child>
            <child>
              <object class="MemoryCellRow"/>
            </child>
            <child>
              <object class="MemoryCellRow"/>
            </child>
            <child>
              <object class="MemoryCellRow"/>
            </child>
            <child>
              <object class="MemoryCellRow"/>
            </child>
          </object>
        </child>
      </object>
    </child>
  </template>
</interface>
