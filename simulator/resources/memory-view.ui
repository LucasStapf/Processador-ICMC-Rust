<?xml version='1.0' encoding='UTF-8'?>
<!-- Created with Cambalache 0.16.0 -->
<interface>
  <!-- interface-description Utilizado para mostrar o conteúdo da memória do Processador. -->
  <requires lib="gtk" version="4.12"/>
  <template class="MemoryView" parent="GtkBox">
    <property name="css-classes">card</property>
    <property name="orientation">vertical</property>
    <property name="valign">center</property>
    <child>
      <object class="GtkBox" id="HeaderMemory">
        <property name="css-classes">card</property>
        <property name="hexpand">True</property>
        <property name="margin-bottom">10</property>
        <child>
          <object class="GtkLabel">
            <property name="halign">center</property>
            <property name="hexpand">True</property>
            <property name="label">&lt;b&gt;Memory&lt;/b&gt;</property>
            <property name="margin-bottom">10</property>
            <property name="margin-top">10</property>
            <property name="use-markup">True</property>
          </object>
        </child>
      </object>
    </child>
    <child>
      <object class="GtkScrolledWindow" id="scrolled_memory_view">
        <property name="focusable">True</property>
        <property name="hscrollbar-policy">never</property>
        <property name="margin-end">4</property>
        <property name="margin-start">4</property>
        <property name="propagate-natural-height">True</property>
        <property name="vexpand">True</property>
        <child>
          <object class="GtkBox" id="box_memory_cells">
            <property name="orientation">vertical</property>
            <child>
              <object class="MemoryCellRow"/>
            </child>
            <child>
              <object class="MemoryCellRow"/>
            </child>
            <child>
              <object class="MemoryCellRow"/>
            </child>
            <child>
              <object class="MemoryCellRow"/>
            </child>
            <child>
              <object class="MemoryCellRow"/>
            </child>
            <child>
              <object class="MemoryCellRow"/>
            </child>
            <child>
              <object class="MemoryCellRow"/>
            </child>
            <child>
              <object class="MemoryCellRow"/>
            </child>
          </object>
        </child>
      </object>
    </child>
    <child>
      <object class="GtkActionBar">
        <property name="hexpand">True</property>
        <property name="vexpand">True</property>
        <child type="center">
          <object class="GtkBox">
            <property name="css-classes">linked</property>
            <property name="halign">center</property>
            <child>
              <object class="GtkSearchEntry" id="search_addr">
                <property name="css-classes">body
numeric</property>
                <property name="placeholder-text">Address</property>
              </object>
            </child>
            <child>
              <object class="GtkButton">
                <property name="icon-name">go-down-symbolic</property>
              </object>
            </child>
            <child>
              <object class="GtkButton">
                <property name="icon-name">go-up-symbolic</property>
              </object>
            </child>
            <child>
              <object class="GtkMenuButton">
                <property name="direction">up</property>
                <property name="icon-name">configure-symbolic</property>
              </object>
            </child>
          </object>
        </child>
      </object>
    </child>
  </template>
</interface>
